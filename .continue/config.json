{
  "models": [
    {
      "title": "GPT-4 (OpenAI)",
      "provider": "openai",
      "model": "gpt-4",
      "apiKey": "",
      "contextLength": 8192
    },
    {
      "title": "GPT-3.5 Turbo",
      "provider": "openai",
      "model": "gpt-3.5-turbo",
      "apiKey": "",
      "contextLength": 4096
    },
    {
      "title": "Claude 3.5 Sonnet (Anthropic)",
      "provider": "anthropic",
      "model": "claude-3-5-sonnet-20241022",
      "apiKey": "",
      "contextLength": 200000
    }
  ],
  "tabAutocompleteModel": {
    "title": "Tab Autocomplete",
    "provider": "openai",
    "model": "gpt-3.5-turbo",
    "apiKey": ""
  },
  "embeddingsProvider": {
    "provider": "openai",
    "model": "text-embedding-ada-002",
    "apiKey": ""
  },
  "customCommands": [
    {
      "name": "fix",
      "prompt": "Please fix the following code:\n\n{{{ input }}}\n\nProvide the corrected code and explain what was wrong."
    },
    {
      "name": "explain",
      "prompt": "Please explain the following code in detail:\n\n{{{ input }}}\n\nBreak down what it does, how it works, and any important patterns or dependencies."
    },
    {
      "name": "refactor",
      "prompt": "Please refactor the following code to improve readability, maintainability, and performance:\n\n{{{ input }}}\n\nExplain what changes you made and why."
    },
    {
      "name": "test",
      "prompt": "Please write comprehensive unit tests for the following code:\n\n{{{ input }}}\n\nInclude edge cases and explain the test strategy."
    },
    {
      "name": "docs",
      "prompt": "Please generate documentation for the following code:\n\n{{{ input }}}\n\nInclude JSDoc comments, parameter descriptions, return values, and usage examples."
    },
    {
      "name": "review",
      "prompt": "Please perform a code review on the following:\n\n{{{ input }}}\n\nLook for bugs, security issues, performance problems, and style inconsistencies. Provide specific suggestions."
    }
  ],
  "contextProviders": [
    {
      "name": "code",
      "params": {}
    },
    {
      "name": "docs",
      "params": {}
    },
    {
      "name": "diff",
      "params": {}
    },
    {
      "name": "terminal",
      "params": {}
    },
    {
      "name": "problems",
      "params": {}
    },
    {
      "name": "folder",
      "params": {}
    },
    {
      "name": "codebase",
      "params": {}
    }
  ],
  "slashCommands": [
    {
      "name": "edit",
      "description": "Edit code in the current file"
    },
    {
      "name": "comment",
      "description": "Add comments to code"
    },
    {
      "name": "share",
      "description": "Share the current conversation"
    },
    {
      "name": "cmd",
      "description": "Generate a shell command"
    },
    {
      "name": "commit",
      "description": "Generate a git commit message"
    }
  ],
  "allowAnonymousTelemetry": false,
  "docs": [
    {
      "title": "React Documentation",
      "startUrl": "https://react.dev"
    },
    {
      "title": "Node.js Documentation",
      "startUrl": "https://nodejs.org/en/docs/"
    },
    {
      "title": "Express.js Documentation",
      "startUrl": "https://expressjs.com/"
    },
    {
      "title": "MongoDB Documentation",
      "startUrl": "https://www.mongodb.com/docs/"
    },
    {
      "title": "Mongoose Documentation",
      "startUrl": "https://mongoosejs.com/docs/"
    }
  ],
  "experimental": {
    "modelRoles": {
      "chat": "GPT-4 (OpenAI)",
      "autocomplete": "Tab Autocomplete",
      "edit": "GPT-4 (OpenAI)"
    }
  }
}
